{
    "name"         : "jbzoo/skeleton-php",
    "type"         : "library",
    "description"  : "__DESCRIPTION__",
    "license"      : "MIT",

    "authors"      : [
        {
            "name"  : "Denis Smetannikov",
            "email" : "admin@jbzoo.com",
            "role"  : "lead"
        }
    ],

    "require"      : {
        "php" : ">=7.4"
    },

    "require-dev"  : {
        "jbzoo/toolbox-dev" : "^4.0.1"
    },

    "autoload"     : {
        "psr-4" : {
            "JBZoo\\__NS__\\" : "src"
        }
    },

    "autoload-dev" : {
        "classmap" : ["tests"]
    },

    "config"       : {
        "optimize-autoloader" : true
    },

    "extra"        : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    }
}
